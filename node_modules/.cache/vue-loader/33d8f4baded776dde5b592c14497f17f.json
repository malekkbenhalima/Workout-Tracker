8 shadow-md cursor-pointer\" :to=\"{name: 'View-Workout', params: {workoutId: workout.id}}\" v-for=\"(workout,index) in data\" :key=\"index\">\n\n        <!-- Cardio Img -->\n        <img v-if=\"workout.workoutType === 'cardio'\" src=\"../assets/images/running-light-green.png\" alt=\"\" class=\"h-24 w-auto\">\n\n        <!-- Strength Training -->\n        <img v-else src=\"../assets/images/dumbbell-light-green.png\" alt=\"\" class=\"h-24 w-auto\">\n\n        <p class=\"mt-6 py-1 px-3 text-xs text-white bg-at-light-green shadow-md rounded-lg\">\n          {{ workout.workoutType }}\n        </p>\n\n        <h1 class=\"mt-9 mb-2 text-center text-xl text-at-light-green\">\n          {{ workout.workoutName }}\n        </h1>\n\n      </router-link>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport {ref} from 'vue';\nimport {supabase} from \"../supabase/init\";\n\nexport default {\n  name: \"Home\",\n  components: {},\n  setup() {\n    // Create data / vars\n    const data = ref([]);\n    const dataLoaded = ref(null);\n\n    // Get data\n    const getData = async () => {\n      try {\n        const {data: workouts, error} = await supabase.from('workouts').select('*');\n        if (error) throw error;\n        data.value = workouts;\n        dataLoaded.value = true;\n      } catch (error) {\n        console.warn(error.message)\n      }\n    }\n\n    // Run data function\n    getData();\n\n    return {data, dataLoaded};\n  },\n};\n</script>\n"]}]}